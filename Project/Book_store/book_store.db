.open book_store.db

-- CREATE TABLE STORES --
CREATE TABLE IF NOT EXISTS stores(
	store_id int,
	store_name text,
	location text,
	phone text
);

CREATE TABLE IF NOT EXISTS books(
	book_id int,
	title text,
	author text,
	price real
);

CREATE TABLE IF NOT EXISTS customers(
	customer_id int,
	customer_firstname text,
	customer_lastname text,
	country,
	phone text,
	email text
);

CREATE TABLE IF NOT EXISTS invoices(
	invoice_id int,
	customer_id int,
	store_id int,
	employee_id int,
	book_id int,
	order_id int,
	quantity int,
	invoice_date text
);

CREATE TABLE IF NOT EXISTS employees(
	employee_id int,
	employee_firstname text,
	employee_lastname text,
	position text,
	phone text,
	email text,
	store_id int
);

CREATE TABLE IF NOT EXISTS orders(
	order_id int,
	order_date text,
	invoice_id int,
	book_id int,
	quantity int,
	customer_id int
);


